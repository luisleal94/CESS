	<queryString>
		<![CDATA[SELECT
     Consulta.`idConsulta` AS Consulta_idConsulta,
     Consulta.`Fecha` AS Consulta_Fecha,
     Consulta.`ExploracionF` AS Consulta_ExploracionF,
     Consulta.`Padecimiento` AS Consulta_Padecimiento,
     Consulta.`Diagnostico` AS Consulta_Diagnostico,
     Consulta.`Estudios` AS Consulta_Estudios,
     Consulta.`Laboratorio` AS Consulta_Laboratorio,
     Consulta.`Gabinete` AS Consulta_Gabinete,
     Consulta.`Tratamiento` AS Consulta_Tratamiento,
     Consulta.`Medico` AS Consulta_Medico,
     Consulta.`Costo` AS Consulta_Costo,
     Consulta.`Tipo` AS Consulta_Tipo,
     Consulta.`Nombre` AS Consulta_Nombre,
     Consulta.`Demanda` AS Consulta_Demanda,
     Consulta.`Folio` AS Consulta_Folio,
     Consulta.`Especialidad` AS Consulta_Especialidad,
     Consulta.`IdPaciente` AS Consulta_IdPaciente,
     Consulta.`IdMedico` AS Consulta_IdMedico,
     PERSONAL.`ID_USER` AS PERSONAL_ID_USER,
     PERSONAL.`NOMBRE` AS PERSONAL_NOMBRE,
     PERSONAL.`APELLIDO` AS PERSONAL_APELLIDO,
     PERSONAL.`PASSWORD` AS PERSONAL_PASSWORD,
     PERSONAL.`telefono` AS PERSONAL_telefono,
     PERSONAL.`Gerarquia` AS PERSONAL_Gerarquia,
     PERSONAL.`Cedula` AS PERSONAL_Cedula,
     PERSONAL.`Username` AS PERSONAL_Username,
     PERSONAL.`Especialidad` AS PERSONAL_Especialidad
FROM
     `Consulta` Consulta,
     `PERSONAL` PERSONAL
where
      PERSONAL.`ID_USER`=$P{Medico} and
      Consulta.`Fecha`=$P{Fecha} and
      Consulta.`IdPaciente`=$P{Paciente} and
      Consulta.`IdMedico`=$P{Medico}]]>
	</queryString>
